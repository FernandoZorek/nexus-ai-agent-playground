{
  "name": "nexus-ai-agent-playground",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "report": "tsx src/scripts/dashboard.ts",
    "export": "tsx src/scripts/export-logs.ts",
    "clean:db": "rm -f nexus_brain.db nexus_brain.db-wal nexus_brain.db-shm",
    "reset": "npm run clean:db && npm run dev"
  },
  "keywords": [
    "ai-agent",
    "ai",
    "ai-telemetry",
    "agent",
    "gemini-api",
    "gpt-4o",
    "rag-testing",
    "history-compression",
    "llm-ops",
    "typescript",
    "google-gemini",
    "langchain",
    "enterprise-ai",
    "sandbox",
    "prompt-engineering",
    "llm"
  ],
  "author": "Fernando Zorek <fernadnozorek@gmail.com>",
  "license": "MIT",
  "description": "Advanced AI Agent playground with Gemini/GPT integration, supporting mocked/production RAG, history compression, and detailed data provenance for logic validation.",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openai": "^6.15.0",
    "zod": "^4.3.4",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
